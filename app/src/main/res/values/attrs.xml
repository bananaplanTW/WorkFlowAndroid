<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="HListView">
        <attr name="android:entries" />
        <attr name="android:divider" />
        <attr name="hlv_dividerWidth" format="dimension" />
        <attr name="hlv_headerDividersEnabled" format="boolean" />
        <attr name="hlv_footerDividersEnabled" format="boolean" />
        <attr name="hlv_overScrollHeader" format="reference|color" />
        <attr name="hlv_overScrollFooter" format="reference|color" />
        <attr name="hlv_measureWithChild" format="integer" />
    </declare-styleable>

    <declare-styleable name="ExpandableLayout">
        <attr name="el_headerLayout" format="reference"/>
        <attr name="el_headerHeight" format="dimension" />
        <attr name="el_contentLayout" format="reference" />
        <attr name="el_duration" format="integer" />
    </declare-styleable>

    <declare-styleable name="TitleEditText">
        <attr name="tet_title" format="string" />
        <attr name="tet_contentHint" format="string" />
        <attr name="tet_contentInputType" format="enum">
            <enum name="none" value="0" />
            <enum name="text" value="1" />
            <enum name="number" value="2" />
        </attr>
        <attr name="tet_contentMinLines" format="integer" />
        <attr name="tet_contentSingleLine" format="boolean" />
        <attr name="tet_contentBackground" format="reference" />
    </declare-styleable>

</resources>