<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_selected="true">
        <shape android:shape="oval">
            <solid android:color="@color/worker_pager_indicator_selected_color" />
            <stroke
                android:width="@dimen/worker_pager_indicator_stroke_width"
                android:color="@color/worker_pager_indicator_stroke_color" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <solid android:color="@color/worker_pager_indicator_normal_color" />
            <stroke
                android:width="@dimen/worker_pager_indicator_stroke_width"
                android:color="@color/worker_pager_indicator_stroke_color" />
        </shape>
    </item>
</selector>




